<% if @booking.errors.any? %>
  $('#errors').html("<%= j render('shared/errors', resource: @booking) %>");
<% else %>
  $('#modal').modal('hide');
  $.notify({ message: 'Reserva realizada com sucesso.' }, { type: 'success' });

  $.ajax({
    url: "/bookings?date=<%= @booking.date %>",
    dataType: 'script'
  });
<% end %>
